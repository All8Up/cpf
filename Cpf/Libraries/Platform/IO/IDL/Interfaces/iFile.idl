import iUnknown from GOM;
import StreamAccess from IO;
module CPF::IO;

interface iStream;

interface iFile : GOM::iUnknown
{
	event OnComplete = void(result value);
	void Exists(const char* name, OnComplete cb);
	void Delete(const char* name, OnComplete cb);
	event OnCreate = void(result value, iStream* stream);
	void Create(const char* name, StreamAccess access, OnCreate cb);
}
